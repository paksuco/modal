{
    "name": "paksuco/modal",
    "description": "A livewire modal component - WIP",
    "keywords": [
        "laravel",
        "livewire",
        "modal",
        "component"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Taha PAKSU",
            "email": "tpaksu@gmail.com",
            "homepage": "https://www.rettica.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/config": "^7.0|^8.0",
        "livewire/livewire": "^2.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Paksuco\\Modal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Paksuco\\Modal\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Paksuco\\Modal\\ModalServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
